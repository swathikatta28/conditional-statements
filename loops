<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
   <script>

    // Loop 1: Print numbers 1 through 10
    for(let i=1; i<=10; i++){
        document.write(i, "<br>");
    }

    // Loop 2: Print numbers 1, 3, 5, 7 (odd numbers)
    for(let i=1; i<=7; i+=2){
        document.write(i, "<br>");
    }

    // Loop 3: Print numbers 10 through 15
    for(let i=10; i<=15; i++){
        document.write(i, "<br>");
    }

    // Loop 4: Print multiplication table for 9
    for(let i=1; i<=10; i++){
        document.write(`9 x ${i} = ${i*9}`, "<br>");
    }

    // Loop 5: Print squares of numbers 1 through 10
    for(let i=1; i<=10; i++){
        document.write(i*i, "<br>");
    }

    // Loop 6: Print "hi" 5 times
    for(let i=0; i<5; i++){
        document.write("hi", "<br>");
    }

   </script>
</body>
</html>
